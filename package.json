{
   "name": "@bekit/scrypted-prometheus",
   "version": "0.0.1",
   "repository": {
      "type": "git",
      "url": "https://github.com/bekriebel/scrypted-prometheus.git"
   },
   "description": "A Scrypted plugin to export event metrics for Prometheus",
   "author": "Brint E. Kriebel",
   "license": "Apache-2.0",
   "scripts": {
      "prepublishOnly": "NODE_ENV=production scrypted-webpack",
      "prescrypted-vscode-launch": "scrypted-webpack",
      "scrypted-vscode-launch": "scrypted-deploy-debug",
      "scrypted-deploy-debug": "scrypted-deploy-debug",
      "scrypted-debug": "scrypted-debug",
      "scrypted-deploy": "scrypted-deploy",
      "scrypted-webpack": "scrypted-webpack"
   },
   "keywords": [
      "prometheus",
      "metrics",
      "scrypted",
      "plugin"
   ],
   "scrypted": {
      "name": "Prometheus Event Metrics",
      "type": "Device",
      "singleInstance": true,
      "interfaces": [
         "HttpRequestHandler"
      ],
      "variables": {
         "allEvents": "builtin:events"
      }
   },
   "devDependencies": {
      "@scrypted/sdk": "0.0.9"
   },
   "dependencies": {
      "prom-client": "^11.2.1"
   }
}
